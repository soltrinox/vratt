FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/SpeechKit"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/ApiAI" "${PODS_ROOT}/Headers/Public/GVRSDK" "${PODS_ROOT}/Headers/Public/SpeechKit"
LIBRARY_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/ApiAI" $(inherited) "${PODS_ROOT}/GVRSDK/Libraries"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/ApiAI" -isystem "${PODS_ROOT}/Headers/Public/GVRSDK" -isystem "${PODS_ROOT}/Headers/Public/SpeechKit"
OTHER_LDFLAGS = $(inherited) -ObjC -l"ApiAI" -l"CardboardSDK_arm64" -l"CardboardSDK_armv7" -l"CardboardSDK_i386" -l"CardboardSDK_x86_64" -l"c++" -framework "AVFoundation" -framework "AudioToolbox" -framework "CFNetwork" -framework "CoreGraphics" -framework "CoreMedia" -framework "CoreMotion" -framework "CoreText" -framework "CoreVideo" -framework "Foundation" -framework "GLKit" -framework "MediaPlayer" -framework "OpenGLES" -framework "QuartzCore" -framework "Security" -framework "SpeechKit" -framework "UIKit"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}/Pods
